prototype(schule.signalwerkCh:CourseView) < prototype(Neos.Neos:Content) {
    test = 'thisisatest'
    requestFormat = ${request.format}
    courseID = ${request.arguments.kurs}

    currentCourse = ${q(site).find('[instanceof schule.signalwerkCh:Course][coursid = "' + request.arguments.kurs +'"]').get(0)}
    currentExecution = ${q(site).find('[instanceof schule.signalwerkCh:CourseExecution][code = "' + request.arguments.durchfuehrung +'"]').get(0)}

    categories = ${q(site).find('[instanceof schule.signalwerkCh:CourseCategory]')}
    # categories = ${q(site).find('[instanceof schule.signalwerkCh:CourseCategory][title = "Vorkurs"]')}
    # categories = ${q(site).find('[instanceof schule.signalwerkCh:Course]').filterPropertiesFromArray('categories', 'efaf294c-2730-446b-892d-1e36e30a0d93')}


    course = Neos.Fusion:Collection {
    	collection = ${q(site).find('[instanceof schule.signalwerkCh:Course][coursid = "' + request.arguments.kurs +'"]')}
      itemName = 'course'
  		itemRenderer = Neos.Fusion:Template {
        templatePath = 'resource://schule.signalwerkCh/Private/Templates/NodeTypes/Course.html'
  			node = ${course}
        executions = Neos.Fusion:Collection {
          collection = ${q(course).find('[instanceof schule.signalwerkCh:CourseExecution]')}
          itemName = 'courseExecution'

          itemRenderer = Neos.Fusion:Template {
            templatePath = 'resource://schule.signalwerkCh/Private/Templates/NodeTypes/CourseExecution.html'
            node = ${courseExecution}
            course = ${course}
            lektionenProVeranstaltung = ${courseExecution.properties.lektionen / courseExecution.properties.veranstaltungen}
          }
        }
      }
    }

		courseItemRenderer = Neos.Fusion:Collection {
      collection = ${q(site).find('[instanceof schule.signalwerkCh:Course]')}
      itemName = 'course'
  		itemRenderer = Neos.Fusion:Template {
        templatePath = 'resource://schule.signalwerkCh/Private/Templates/NodeTypes/CourseViewListItem.html'
  			course = ${course}
        executions = Neos.Fusion:Collection {
          collection = ${q(course).find('[instanceof schule.signalwerkCh:CourseExecution]')}
          itemName = 'courseExecution'

          itemRenderer = Neos.Fusion:Template {
            templatePath = 'resource://schule.signalwerkCh/Private/Templates/NodeTypes/CourseExecutionViewListItem.html'
            courseExecution = ${courseExecution}
          }
        }
        // executions = Neos.Neos:ContentCollection {
        //   nodePath = 'executions'
        // }
  		}
		}
}

